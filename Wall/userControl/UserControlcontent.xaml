<UserControl x:Class="tp2.Wall.UserControlcontent"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:tp2.Wall"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Background="Black">
        <TextBlock Margin="10,5,0,0" x:Name="Title" FontSize="15" FontWeight="DemiBold" Foreground="White">title</TextBlock>
        <TextBlock Margin="10,0" x:Name="Date" Foreground="Gray">Date</TextBlock>
        <TextBlock Margin="10,10" x:Name="Content" Foreground="White">Content</TextBlock>
    </StackPanel>
</UserControl>
